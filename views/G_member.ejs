                <form class="form-inline mr-auto w-100 navbar-search">
                  <div class="input-group">
                    <input type="text" class="form-control bg-light border-0 small" placeholder="Search for..."
                      aria-label="Search" aria-describedby="basic-addon2">
                    <div class="input-group-append">
                      <button class="btn btn-primary" type="button">
                        <i class="fas fa-search fa-sm"></i>
                      </button>
                    </div>
                  </div>
                </form>
              </div>
            </li>

            <!-- Nav Item - User Information -->
            <%if(nickname!=""){%>
              <li class="nav-item dropdown no-arrow">
                <a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown"
                  aria-haspopup="true" aria-expanded="false">
                  <span class="mr-2 d-none d-lg-inline text-gray-600 small"><%=nickname%></span>
                </a>
                <!-- Dropdown - User Information -->
                <div class="dropdown-menu dropdown-menu-right shadow animated--grow-in" aria-labelledby="userDropdown">
                  <a class="dropdown-item" href="/member/myEdit">
                    <i class="fas fa-user fa-sm fa-fw mr-2 text-gray-400"></i>
                    내정보수정
                  </a>
                  <a class="dropdown-item" href="/animal/myPet">
                    <i class="fas fa-list fa-sm fa-fw mr-2 text-gray-400"></i>
                    내동물관리
                  </a>
                  <a class="dropdown-item" href="/board/myBoard">
                    <i class="fas fa-list fa-sm fa-fw mr-2 text-gray-400"></i>
                    내글관리
                  </a>
                  <a class="dropdown-item" href="/diagnosis/myDiagnosis">
                    <i class="fas fa-list fa-sm fa-fw mr-2 text-gray-400"></i>
                    내진료관리
                  </a>
                  <a class="dropdown-item" href="/member/logout"  >
                    <i class="fas fa-sign-out-alt fa-sm fa-fw mr-2 text-gray-400"></i>
                    로그아웃
                  </a>
  
                </div>
              </li>
              <%}else{%>
                <li class="nav-item dropdown no-arrow">
                  <a class="nav-link dropdown-toggle" href="/html/login.html" role="button">
                    로그인
                  </a>
                </li>
                <%}%>
              </ul>

        </nav>
        <!-- End of Topbar -->

        <!-- Begin Page Content -->
        <div class="container-fluid">
          
          <!-- DataTales Example -->
          <div class="card shadow mb-4">
            <div class="card-header py-3">
              <h6 class="m-0 font-weight-bold text-primary">회원관리</h6>
            </div>
            <div class="card-body">
              <a href="/html/G_memberRegist.html" class="btn btn-primary btn-icon-split">
                <span class="icon text-white-50">
                  <i class="fas fa-flag"></i>
                </span>
                <span class="text">회원등록</span>
              </a>
              <hr>
                <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                  <thead>
                    <tr>
                      <th>아이디</th>
                      <th>비밀번호</th>
                      <th>이름</th>
                      <th>이메일</th>
                      <th>연락처</th>
                      <th>닉네임</th>
                      <th>가입날짜</th>
                    </tr>
                  </thead>
                  <tbody>
                    <%for(var i=0;i<memberArray.length;i++){%>
                      <%var member = memberArray[i]%>
                    <tr>
                      <td><a href="/member/detail?member_id=<%=member.member_id%>">
                        <%=member.member_id%></a></td>
                        <td><%=member.password%></td>
                        <td><%=member.member_name%></td>
                        <td><%=member.email_id%></td>
                        <td><%=member.member_phone%></td>
                        <td><%=member.member_nickname%></td>
                        <td><%=member.regdate%></td>
                    </tr>
                    <%}%>
                  </tbody>
                </table>
                <%if(startPage>1){%>
                  <a href="/member/list?currentPage=1"></a>
                      <%}%>
                      <%if(currentPage>1){%>
                      <a href="/member/list?currentPage=<%=currentPage-1%>"><</a>
                          <%}%>
                          <%for(var i=startPage; i<=endPage;i++){%>
                          <%if(i==currentPage){%>
                          <%=i%>
                          <%}else{%>
                            <a href="/member/list?currentPage=<%=i%>%>"><%=i%></a>
                          <%}%>
                          <%}%>
                          <%if(currentPage<totalPage){%>
                          <a href="/member/list?currentPage=<%=currentPage+1%>">></a>
                          <%}%>
                          <%if(endPage<totalPage){%>
                          <a href="/member/list?currentPage=<%=totalPage%>">>></a>
                          <%}%>
              </div>
            </div>
          </div>

        </div>
        <!-- /.container-fluid -->

        

      </div>
      <!-- End of Main Content -->

